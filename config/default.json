{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 1000
  },
  "authentication": {
    "secret": "supersecret",
    "strategies": [
      "jwt"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "type": "access"
      },
      "audience": "https://yourdomain.edu/",
      "subject": "Office hours app",
      "issuer": "Your institution",
      "algorithm": "HS256",
      "expiresIn": "2h"
    },
    "cookie": {
      "enabled": true,
      "name": "feathers-jwt",
      "httpOnly": true,
      "secure": false
    }
  },
  "mongodb": "mongodb://localhost:27017/officehours",
  "http": "http://",
  "CAS": {
    "baseURL": "https://yourcasinstance.yourdomain.edu/",
    "validationPath": "/serviceValidate"
  },
  "tokens": {
    "max": 5
  },
  "autologin": {
    "fakeUser0": "populateWithAUserId"
  }
}
